<!DOCTYPE html>
<html>
<head>
  <title>ReactToCheese</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header class='main'>
    <div class='row'>
      <div class='col-sm-4'>
        <h1><%= link_to "Cheese Head", root_path %></h1>
      </div>
    </div>
  </header>
  <main>
    <%= react_component("Navigation") %>
    <div class='container'>
      <div class='row'>
        <div class='content col-lg-12'>
          <%= yield %>
        </div>
      </div>
    </div>
  </main>
</body>
</html>
